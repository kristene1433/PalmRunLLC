# EmailJS Configuration
# Copy this file to .env and fill in your actual values

# EmailJS Service ID (one service for all templates)
REACT_APP_EMAILJS_SERVICE_ID=service_your_service_id

# EmailJS Template IDs (separate for each email type)
REACT_APP_EMAILJS_CONTACT_TEMPLATE_ID=template_your_contact_template_id
REACT_APP_EMAILJS_LEASE_TEMPLATE_ID=template_your_lease_template_id

# EmailJS Public Key
REACT_APP_EMAILJS_PUBLIC_KEY=user_your_public_key

# Stripe Configuration
REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key

# Note: All environment variables in React must start with REACT_APP_
# These will be embedded in your build, so they're safe for client-side use
